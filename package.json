{
  "name": "@billjs/magot",
  "version": "0.0.1-alpha.1",
  "description": "A cool and easy UI framework of customizable themes and React-based.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "es",
    "lib",
    "release",
    "lib.d.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "prepublishOnly": "npm run lint && npm run build",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -r @babel/register --config webpack.config.dev.js --progress --colors",
    "release": "cross-env NODE_ENV=production webpack -r @babel/register --config webpack.config.js --progress",
    "compile:es": "node -r @babel/register scripts/build/es.js",
    "compile:lib": "node -r @babel/register scripts/build/lib.js",
    "compile": "npm run compile:es && npm run compile:lib || exit 1",
    "build": "node -r @babel/register scripts/build/release.js || exit 1",
    "fix:prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,css,less,json,yml,yaml,md,mdx}\"",
    "fix:es": "eslint --ext .js,.ts,.jsx,.tsx ./ --cache --quiet --fix",
    "fix:style": "stylelint \"src/**/*.{css,less}\" --syntax less --fix",
    "fix": "npm run fix:prettier && npm run fix:es && npm run fix:style || exit 1",
    "lint:prettier": "prettier --list-different \"**/*.{ts,tsx,js,jsx,css,less,json,yml,yaml,md,mdx}\"",
    "lint:es": "eslint --ext .js,.ts,.jsx,.tsx ./ --cache --quiet",
    "lint:style": "stylelint \"src/**/*.{css,less}\" --syntax less",
    "lint": "npm run lint:prettier && npm run lint:es && npm run lint:style || exit 1",
    "docz:dev": "cross-env DEBUG=true docz dev",
    "docz:build": "docz build",
    "create:component": "node -r @babel/register scripts/creator/component/index.js",
    "create:test": "node -r @babel/register scripts/creator/test/index.js",
    "test": "jest --no-cache",
    "test:dev": "jest --no-cache --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/billjs/magot.git"
  },
  "keywords": [
    "Magot",
    "UI",
    "React",
    "Theme"
  ],
  "contributors": [
    "billjs"
  ],
  "author": "billjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/billjs/magot/issues"
  },
  "homepage": "https://billjs.github.io/magot/",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@emotion/core": "^10.0.10",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.2",
    "@types/jest": "^24.0.13",
    "@types/less": "^3.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.0.2",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-router": "^5.0.0",
    "@types/react-router-dom": "^4.3.3",
    "@types/shelljs": "^0.8.5",
    "@types/sinon": "^7.0.11",
    "@types/webpack": "^4.4.31",
    "@types/webpack-dev-server": "^3.1.5",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "docz": "^1.2.0",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^1.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "^0.6.0",
    "mkdirp": "^0.5.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-each": "^0.10.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.1",
    "postcss-reporter": "^6.0.1",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "shelljs": "^0.8.3",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "ts-loader": "^6.0.1",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^3.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
